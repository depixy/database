{
  "name": "@depixy/database",
  "version": "2.3.1",
  "description": "",
  "main": "./lib/cjs/index.js",
  "typings": "./lib/cjs/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf lib && prisma generate",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module node16 --outDir lib/esm",
    "build:cjs": "tsc --module CommonJS --outDir lib/cjs",
    "lint": "eslint --ext .ts src",
    "generate": "prisma generate",
    "migrate": "prisma migrate",
    "migrate:create": "prisma migrate dev --create-only",
    "studio": "prisma studio",
    "postinstall": "npx prisma generate"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "keywords": [
    "depixy",
    "depixy-plugin"
  ],
  "homepage": "https://github.com/depixy/database#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/depixy/database.git"
  },
  "bugs": {
    "url": "https://github.com/depixy/database/issues"
  },
  "author": "Joshua Avalon",
  "license": "Apache-2.0",
  "files": [
    "src",
    "lib",
    "prisma"
  ],
  "dependencies": {
    "@prisma/client": "^3.15.1",
    "fastify-plugin": "^3.0.1"
  },
  "devDependencies": {
    "@joshuaavalon/eslint-config-typescript": "^6.1.4",
    "@types/node": "^16.11.39",
    "eslint": "^8.17.0",
    "fastify": "^4.0.1",
    "prettier": "^2.6.2",
    "prisma": "^3.15.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3"
  }
}
